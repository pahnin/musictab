.player
	%span#playtoggle
	%span#gutter
		%span#loading
		%span#handle.ui-slider-handle
	%span#timeleft
	
	
.list
	%table
		%thead
			%tr
				%td{width:"30px"}
					Sl.no
				%td{width:"20%"}
					Title
				%td{width:"20%"}
					Album
				%td{width:"20%"}
					Artist
		%tbody
			- Files.each do |object|
				%tr{:id => object.id+1}
					%td= object.id
					%td= object.title
					%td= object.album
					%td= object.artist
